<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.epi.datasource.dao.FilesDao">


<resultMap type="cn.epi.datasource.entity.Files" id="getResultMap">
		<result column="old_name" property="old_name" />
		<result column="new_name" property="new_name" />
		
	</resultMap>
<select id="getName" resultMap="getResultMap">
		SELECT
		old_name,new_name
		FROM sjlj_file a
		WHERE 0=0 and a.data_resource_id = #{data_resource_id}
	</select>
<insert id="insert">
		insert into sjlj_file
		(old_name,new_name,url,data_resource_id,csv_separator,withheader)
		values(#{old_name},#{new_name},#{url},#{data_resource_id},#{csv_separator},#{withheader})
	</insert>


	<update id="update">
		update sjlj_files set
		old_name=#{old_name},new_name=#{new_name},url=#{url},data_resource_id=#{data_resource_id},csv_separator=#{csv_separator},withheader=#{withheader}
		where data_resource_id=#{data_resource_id}
	</update>

	<delete id="delete">
		delete from sjlj_files where
		data_resource_id=#{data_resource_id}
	</delete>

</mapper>